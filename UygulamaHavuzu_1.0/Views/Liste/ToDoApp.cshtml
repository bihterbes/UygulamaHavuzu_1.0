@{

    
    <title>Todo App Denemesi</title>
}
@model List<YapmaListesi>

    <a asp-controller="Liste" asp-action="add"> Ekle</a>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>İsim</th>
            <th>Yapılacak</th>
            <th>İşlem</th>
        </tr>
    </thead>
    <tbody>


        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Do</td>
                <td><a asp-controller="Liste" asp-action="remove" asp-route-id="@item.Id">Sil</a></td>
                <td><a asp-controller="Liste" asp-action="update" asp-route-id="@item.Id">güncelle</a></td>
            </tr>

        }
    </tbody>

</table>




<div class="custom-control custom-checkbox">
    <input type="checkbox" class="custom-control-input" id="customCheck1">
    <label class="custom-control-label" for="customCheck1">Check this custom checkbox</label>
</div>
